function [J]=mask_messenger(xa,foptions)

ll=[1000 1 0 0 30 30 30 30 0.01 0.01 0.01 0.01 1.1 1.1 1.1 -3.14159265358979 -3.14159265358979 -3.14159265358979
    4000 5 1 1 400 400 400 400 0.99 0.99 0.99 0.99 6 6 6 3.14159265358979 3.14159265358979 3.14159265358979];
 
x=(ll(2,:)-ll(1,:)).*xa+ll(1,:);

[J] = messenger(x,foptions.prob);
